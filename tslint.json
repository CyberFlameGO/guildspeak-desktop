{
  "defaultSeverity": "error",
  "extends": [
    "tslint-config-airbnb",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rules": {
    "semicolon": [
      true,
      "never"
    ],
    "variable-name": false,
    "import-name": false,
    "object-shorthand-properties-first": false
  }
}
