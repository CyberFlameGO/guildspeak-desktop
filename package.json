{
  "name": "guildspeak",
  "version": "0.0.1",
  "description": "",
  "main": "./build/main.bundle.js",
  "scripts": {
    "dev": "concurrently \"cross-env ENV='dev' webpack-dev-server --config webpack.config.renderer.js\" \" cross-env ENV='dev' webpack\"",
    "build": "concurrently \"webpack --config webpack.config.renderer.js\" \"webpack\"",
    "start": "cross-env ENV='dev' electron .",
    "build-win": "npm run build && electron-builder -w -p always",
    "build-mac": "npm run build && electron-builder -m -p always",
    "build-linux": "npm run build && electron-builder -l -p always",
    "lint": "tslint \"src/**/*.ts*\"",
    "lint-fix": "prettier --write \"src/**/*.ts*\" -&& tslint \"src/**/*.ts*\" --fix"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/test/.+\\.spec)\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "node"
    ]
  },
